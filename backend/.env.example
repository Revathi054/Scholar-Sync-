# Copy this file to .env and fill in the values before running the server
# Do NOT commit real secrets to source control

# MongoDB connection string (e.g. mongodb://user:pass@host:port/dbname)
MONGO_URI=

# JWT secret for signing tokens
JWT_SECRET=

# Optional: token expiry (e.g. 7d, 12h)
JWT_EXPIRES_IN=7d

# Server port
PORT=5000

# OpenAI API key for AI chatbot (keep secret)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-3.5-turbo
